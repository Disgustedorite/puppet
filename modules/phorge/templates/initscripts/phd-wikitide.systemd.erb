[Unit]
Description=phorge-phd for wikitide
After=syslog.target network.target mysql.service
Before=nginx.service

[Service]
Type=forking
User=www-data
Group=www-data
Environment=PHABRICATOR_ENV=conf/wikitide
ExecStart=/srv/phorge/phorge/bin/phd start
ExecStop=/srv/phorge/phorge/bin/phd stop --force
RestartSec=10
Restart=always

[Install]
WantedBy=multi-user.target
